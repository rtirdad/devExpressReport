<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <connectionStrings>
        <add name="JsonConnection" connectionString="Uri=https://raw.githubusercontent.com/DevExpress-Examples/DataSources/master/JSON/customers.json"
            providerName="JsonSourceProvider" />
        <add name="OData" connectionString="Uri=https://services.odata.org/V3/Northwind/Northwind.svc/;query:$format=json;query:filter=&quot;expression:'CategoryID eg ' + ?parameter1&quot;;path:PathParameter1=Categories"
            providerName="JsonSourceProvider" />
